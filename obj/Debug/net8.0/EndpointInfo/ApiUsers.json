{
  "openapi": "3.0.1",
  "info": {
    "title": "ApiUsers",
    "version": "1.0"
  },
  "paths": {
    "/card": {
      "post": {
        "tags": [
          "ControllerCard"
        ],
        "parameters": [
          {
            "name": "email",
            "in": "query",
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/AddCardDTO"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/AddCardDTO"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/AddCardDTO"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/Users/All": {
      "get": {
        "tags": [
          "User"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/User/CreateUser/user/create": {
      "post": {
        "tags": [
          "User"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateDto"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateDto"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/CreateDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/User/InicisarSesion/user/inicio_sesion": {
      "post": {
        "tags": [
          "User"
        ],
        "parameters": [
          {
            "name": "data",
            "in": "query",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "AddCardDTO": {
        "type": "object",
        "properties": {
          "usuario_id": {
            "type": "integer",
            "format": "int32"
          },
          "card_id": {
            "type": "integer",
            "format": "int32"
          },
          "card_number": {
            "type": "string",
            "nullable": true
          },
          "vencimiento": {
            "type": "string",
            "format": "date-time"
          },
          "cvc": {
            "type": "string",
            "nullable": true
          },
          "titular": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "CreateDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "nullable": true
          },
          "email": {
            "type": "string",
            "nullable": true
          },
          "phone": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}